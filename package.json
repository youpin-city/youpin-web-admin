{
  "name": "youpin-web-admin",
  "version": "0.0.0",
  "description": "Mafueng Web Admin",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "node bin/start.js && parallelshell 'pm2 logs' 'npm run watch'",
    "stop:dev": "node bin/stop.js && pm2 ls",
    "build:css": "echo '=> build:css' && node-sass --source-map true --output-style expanded -o public/css/ src/assets/css/ && postcss -u autoprefixer -r public/css/*.css",
    "watch:css": "onchange 'src/assets/css/**/*.scss' -v -- npm run build:css",
    "prebuild:js": "echo 'prebuild js' || mkdir -p public/js",
    "build:js": "echo '=> build:js' && browserify src/assets/js/main.js --debug -t [ babelify ] | exorcist public/js/main.js.map > public/js/main.js && browserify src/assets/js/dep.js --debug -t [ babelify ] | exorcist public/js/dep.js.map > public/js/dep.js",
    "postbuild:js": "uglifyjs public/js/main.js -c warnings=false -m -o public/js/main.min.js && uglifyjs public/js/dep.js -c warnings=false -m -o public/js/dep.min.js",
    "watch:js": "onchange 'src/assets/js/**/*.js' -v -- npm run build:js",
    "build:tag": "echo '=> build:tag' && riot --template pug --style scss src/assets/templates/ public/js/tag.build.js && babel public/js/tag.build.js --compact false -o public/js/tag.js && uglifyjs public/js/tag.js -c warnings=false -m -o public/js/tag.min.js && rm -f public/js/tag.build.js",
    "watch:tag": "onchange 'src/assets/templates/**/*.tag' -v -- npm run build:tag",
    "build": "echo '=> building...' && npm run build:css -s && npm run build:tag -s && npm run build:js -s",
    "watch": "parallelshell 'npm run watch:tag -s' 'npm run watch:css -s' 'npm run watch:js -s'",
    "lint": "echo '=> linting' && eslint . && echo 'lint ok.'",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive ./test/server && NODE_ENV=test wdio",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "version": "npm run build && git add -u",
    "postversion": "npm run push",
    "push": "git push && git push --tags",
    "job": "node -e \"console.log('Run: npm run job:start -- <job_name>\\n\\n' + require('fs').readdirSync('./src/lib/jobs/').filter(path => path.indexOf('.js') >= 0).map(path => path.replace(/.js$/gi, '')).join('\\n'));\"",
    "job:start": "babel-node -e \"var j=require('./src/lib/jobs/' + process.argv[4]);j.default();\"",
    "cron": "babel-node -e \"require('./src/lib/cron').default.start();",
    "deploy:production": "pm2 deploy ecosystem.json production --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/youpin-city/youpin-web-admin.git"
  },
  "author": "YouPin <contact@youpin.city>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "connect-flash": "^0.1.1",
    "convict": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-session": "^1.13.0",
    "express-useragent": "^0.2.4",
    "express-validator": "^2.20.10",
    "faker": "^3.1.0",
    "http-errors": "^1.5.0",
    "i18n": "^0.8.3",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.6.1",
    "jstransformer-scss": "^0.1.3",
    "juice": "^3.0.1",
    "lodash": "^4.13.1",
    "marked": "^0.3.6",
    "mime-types": "^2.1.11",
    "moment": "^2.13.0",
    "node-schedule": "^1.1.1",
    "nodemailer": "^2.6.4",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-wellknown": "^0.2.0",
    "pm2": "^2.0.19",
    "pug": "^2.0.0-beta6",
    "qs": "^6.2.1",
    "query-string": "^4.2.3",
    "riot": "^2.6.5",
    "sanitize-html": "^1.13.0",
    "serve-favicon": "^2.3.0",
    "superagent": "^2.0.0",
    "superagent-bluebird-promise": "^3.0.2",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.10.1",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "dropzone": "^4.3.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "exorcist": "^0.4.0",
    "jquery": "^2.2.1",
    "jquery-serializejson": "^2.6.2",
    "jquery.scrollto": "^2.1.2",
    "js-cookie": "^2.1.3",
    "leaflet": "^0.7.7",
    "materialize-css": "^0.97.8",
    "mocha": "^3.1.2",
    "node-sass": "^3.7.0",
    "onchange": "^2.5.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.2",
    "redux": "^3.5.2",
    "scrollmagic": "^2.0.5",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "slick-carousel": "^1.6.0",
    "slick-lightbox": "^0.2.0",
    "uglify-js": "^2.6.2",
    "wdio-dot-reporter": "0.0.6",
    "wdio-mocha-framework": "^0.5.2",
    "wdio-selenium-standalone-service": "0.0.7",
    "webdriverio": "^4.3.0"
  },
  "browser": {
    "slick-lightbox": "./node_modules/slick-lightbox/dist/slick-lightbox.js"
  }
}
